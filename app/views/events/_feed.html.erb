<%= form_tag url_for(controller: controller.controller_name, action: controller.action_name), remote: true, class: 'search row h2' do %>
	<%= text_field_tag :search, params[:search], placeholder: 'search' %>
	<%= submit_tag '>>' %>
<% end %>
<div class="events feed col" data-search-target="1" data-scroll-content="1">
	<%= render @events %>
</div>