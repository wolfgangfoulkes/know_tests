<div class="item row <%= 'past' if (event.starts_at <= DateTime.now) %>">
    <div class="image-temp"> </div>
    <div class= "col">
      <div class="date h5"> <%= event.starts_at %> </div>

      <div class="br"> </div>

      <%= link_to event.name, event, class: "title h2" %>
      <span class="tags h4"> 
        <% event.tags.first(10).each do |tag| %> 
          <%= "#{tag.name}" %>
        <% end %>
      </span>

      <div class="br"> </div>
      
    </div>
</div>